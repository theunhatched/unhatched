{
  "name": "unhatched",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "lint": "eslint .",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "0.16.0",
    "apollo-server-micro": "2.16.1",
    "classnames": "2.2.6",
    "dataloader": "2.0.0",
    "dotenv": "8.2.0",
    "isomorphic-unfetch": "3.0.0",
    "knex": "0.21.5",
    "micro-cors": "0.1.1",
    "next": "9.5.2",
    "next-absolute-url": "1.2.2",
    "pg": "8.3.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "swr": "0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@philihp/eslint-config": "3.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.11"
  },
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
